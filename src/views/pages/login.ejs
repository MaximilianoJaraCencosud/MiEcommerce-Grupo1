<%- include('../partials/head', {variable : "Login | MiEcommerce"}) %> 
<header class="header">
    <a href="/" class="header__logo">
      <img src="/images/MiEcommerce.png" alt="Es el logo de la empresa" />
    </a>
</header>
<main class="access">
  <div class="access__bg-green"></div>
  <div class="access__container">
    <div class="access__card">
      <h2 class="access__card__title">¡Hola!</h2>
      <form class="access__card__form" action="" method="POST">
        <%- include('../partials/input',{type: 'text', placeholder: 'Tu nombre de usuario' }) %> 
        <%- include('../partials/input',{type: 'password', placeholder: 'Tu contraseña' }) %> 
        <%-include('../partials/button',{text: 'Iniciar Sesión'}) %>
      </form>
    </div>
    <p class="access__text" for="">
      ¿No tienes cuenta? <a class="access__text__link" href="register">Regístrate</a>
    </p>
  </div>
</main>
<%- include('../partials/footer') %>